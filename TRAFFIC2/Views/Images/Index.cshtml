@model IEnumerable< TRAFFIC2.Models.Image>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HomePageId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImagePath1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImagePath2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImagePath3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImagePath4)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HomePageId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagePath1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagePath2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagePath3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImagePath4)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ImageId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ImageId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ImageId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
